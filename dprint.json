{
  "markdown": {
    "emphasisKind": "asterisks"
  },
  "yaml": {
    "preferSingleLine": true,
    "formatComments": true
  },
  "typescript": {
    "preferSingleLine": "true"
  },
  "markup": {
    "printWidth": 120,
    "indentWidth": 4,
    "formatComments": false,
    "html.void.selfClosing": false,
    "svg.selfClosing": true,
    "mathml.selfClosing": true,
    "html.normal.selfClosing": false,
    "preferAttrsSingleLine": false,
    "styleIndent": true,
    "scriptIndent": true
  },
  "malva": {
    "printWidth": 120,
    "indentWidth": 4,
    "quotes": "alwaysSingle",
    "operatorLineBreak": "before",
    "formatComments": true,
    "linebreakInPseudoParens": true,
    "declarationOrder": "smacss",
    "keyframeSelectorNotation": "percentage"
  },
  "excludes": [],
  "plugins": [
    "https://plugins.dprint.dev/markdown-0.20.0.wasm",
    "https://plugins.dprint.dev/g-plane/pretty_yaml-v0.5.1.wasm",
    "https://plugins.dprint.dev/typescript-0.95.13.wasm",
    "https://plugins.dprint.dev/json-0.21.0.wasm",
    "https://plugins.dprint.dev/g-plane/markup_fmt-v0.25.3.wasm",
    "https://plugins.dprint.dev/dockerfile-0.3.3.wasm",
    "https://plugins.dprint.dev/g-plane/malva-v0.15.1.wasm"
  ]
}
